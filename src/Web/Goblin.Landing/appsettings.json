{
  "AllowedHosts": "*",
  "Kestrel": {
    "EndPoints": {
      "Http": {
        "Url": "http://*:7001"
      }
    }
  },
  "IIS": {
    "IsUseIIS": false,
    "IsUseIISIntegration": false
  },
  "Setting": {
    "ApplicationName": "Top Nguyen",
    "ApplicationDescription": "Hi. I am Top Nguyen, I do the work I do because I love it.",
    "GoogleMapKey": ""
  },
  "ElectLog": {
    "IsEnableLogToConsole": true,
    "IsEnableLogToFile": false,
    "JsonFilePath": "Logs/{yyyy-MM-dd}.json",
    "IsLogFullInfo": false,
    "Url": "/developers/logs",
    "AccessKey": "",
    "UnAuthorizeMessage": "You don't have permission to view Log Dashboard, please contact your administrator."
  },
  "ElectSwagger": {
    "IsEnable": true,
    "SwaggerRoutePrefix": "developers/api-docs",
    "Url": "/developers",
    "JsonViewerUrl": "/developers/json-viewer",
    "Title": "API Document",
    "Version": "latest",
    "AccessKey": "",
    "UnAuthorizeMessage": "You don't have permission to view API Document, please contact your administrator.",
    "IsFullSchemaForType": false,
    "IsDescribeAllEnumsAsString": true,
    "IsDescribeAllParametersInCamelCase": true,
    "AuthorName": "",
    "AuthorEmail": "",
    "AuthorWebsite": ""
  },
  "ElectHangfire": {
    "IsEnable": false,
    "IsDisableJobDashboard": false,
    "StatsPollingInterval": 3000,
    "BackToUrl": "/",
    "Url": "/developers/job",
    "AccessKey": "",
    "UnAuthorizeMessage": "You don't have permission to view Log Dashboard, please contact your administrator.",
    "Provider": "SqlServer",
    "HangfireDatabaseConnectionString": "Data Source=.;Initial Catalog=<DbName>;User ID=<UserName>;Password=<Password>;Application Name=Goblin.Landing_Job"
  },
  "ElectHealthCheck": {
    "IsEnable": false,
    "Endpoint": "/health",
    "DbConnectionString": "Data Source=.;Initial Catalog=<DbName>;User ID=<UserName>;Password=<Password>;Application Name=Goblin.Landing_Healthy"
  }
}